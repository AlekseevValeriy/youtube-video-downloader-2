<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="YoTuViLo2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:YoTuViLo2.Views"
    Title="YoTuViLo2">

    <ShellContent
        Title="Download"
        ContentTemplate="{DataTemplate local:DownloadPage}"
        Route="DownloadPage" />
    <ShellContent
        Title="Settings"
        ContentTemplate="{DataTemplate local:SettingsPage}"
        Route="SettingsPage" />
    <ShellContent
        Title="Help"
        ContentTemplate="{DataTemplate local:HelpPage}"
        Route="HelpPage" />

</Shell>
